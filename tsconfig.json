{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@app/*": ["./src/app/*"],
      "@constants": ["./src/constants"],
      "@constants/*": ["./src/constants/*"],
      "@utils": ["./src/utils"],
      "@utils/*": ["./src/utils/*"],
      "@components/*": ["./src/components/*"],
      "@hooks/*": ["./src/hooks/*"],
      "@types": ["./src/types"],
      "@types/*": ["./src/types/*"],
      "@providers/*": ["./src/providers/*"],
      "@src/*": ["./src/*"],
      "@assets/*": ["./assets/*"],
      "@data/*": ["./data/*"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx", ".expo/types/**/*.ts", "expo-env.d.ts", "webpack.config.js"]
}
